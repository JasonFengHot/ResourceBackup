            Redmine #71500 : [bug](拨号盘)*#06#无法查看IMEI码
                拨号盘输入*#06#查看IMEI码
                M:alps/packages/apps/Dialer/src/com/android/dialer/dialpad/DialpadFragment.java
                    private boolean handleIMEIDisplay(Context context) {
                        TelephonyManager telephonyManager = getTelephonyManager();
                        int phoneType = telephonyManager.getCurrentPhoneType();
                        if (phoneType == TelephonyManager.PHONE_TYPE_GSM || phoneType == TelephonyManager.PHONE_TYPE_CDMA) {
                            String imeiStr = telephonyManager.getDeviceId();
                            if (TextUtils.isEmpty(imeiStr)) {
                                imeiStr = context.getResources().getString(R.string.imei_invalid);
                            }
                            AlertDialog.Builder alert = new AlertDialog.Builder(context);
                            if (phoneType == TelephonyManager.PHONE_TYPE_CDMA) {
                                alert.setTitle(R.string.meid);
                                alert.setMessage(imeiStr);
                            } else if (phoneType == TelephonyManager.PHONE_TYPE_GSM) {
                                alert.setTitle(R.string.imei);
                                if (SlotUtils.isGeminiEnabled()) {
                                    List<String> imeis = new ArrayList<String>();
                                    String imei_invalid = context.getResources().getString(R.string.imei_invalid);
                                    for (int slotId : SlotUtils.getAllSlotIds()) {
                                        String imei = TelephonyManagerEx.getDefault().getDeviceId(slotId);
                                        imeis.add(TextUtils.isEmpty(imei) ? imei_invalid : imei);
                                    }
                                    if (FeatureOption.MTK_SINGLE_IMEI) {
                                        alert.setMessage(imeis.get(0));
                                    } else {
                                        alert.setItems(imeis.toArray(new String[imeis.size()]), null);
                                    }
                                }
                            }
                            alert.setPositiveButton(android.R.string.ok, null);
                            alert.setCancelable(false);
                            alert.create().show();
                            return true;
                        }
                        return false;
                    }
                
