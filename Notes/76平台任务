    //zhangqi modified begin
    String packageName = mContext.getPackageName();
    android.util.Log.e("zhangqi1111", "AlertController->createListView(1)->packageName->" + packageName);
    if("com.whatsapp".equalsIgnoreCase(packageName)){
        ArrayList<CharSequence> itemList = new ArrayList<CharSequence>();
        for(CharSequence item : mItems){
            android.util.Log.e("zhangqi1111", "AlertController->createListView(1)->item1->" + item);
            if(item.toString().equalsIgnoreCase("Add shortcut")){
                continue;
            }
            android.util.Log.e("zhangqi1111", "AlertController->createListView(1)->item2->" + item);
            itemList.add(item);
        }
        mItems = itemList.toArray(new String[]{});
    }
    //zhangqi modified end
