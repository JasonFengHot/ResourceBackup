			alps/packages/apps/Launcher3/src/com/android/launcher3/Launcher.java
				if(KeyEvent.KEYCODE_0 <= keyCode && keyCode <= KeyEvent.KEYCODE_9){
					Intent intent = new Intent(Intent.ACTION_DIAL);
					intent.setData(Uri.parse("tel:" + (keyCode - KEYCODE_0)));
					intent.setFlags(Intent.FLAG_ACTIVITY_NEW_TASK);
					try {
						startActivity(intent);
					} catch (ActivityNotFoundException e) {
						Log.w(TAG, "No activity found for android.intent.action.DIAL.");
					}
				}
			
